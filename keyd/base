tab = layer(meta)

leftalt = layer(media)
leftalt = overload(media, esc)

space = layer(nav)
space = overload(nav, space)

leftmeta = layer(fun)
leftmeta = overload(fun, tab)

rightalt = layer(sym)
rightalt = overload(sym, enter)

rightcontrol = layer(num)
rightcontrol = overload(num, backspace)

a = overloadt2(meta, a, 250)
s = overloadt2(alt, s, 250)
d = overloadt2(control, d, 250)
f = overloadt2(shift, f, 250)

j = overloadt2(shift, j, 250)
k = overloadt2(control, k, 250)
l = overloadt2(alt, l, 250)
; = overloadt2(meta, ;, 250)
